local run = require "dromozoa.ubench.run"

local format = string.format

local B = assert(loadfile(arg[1]))()
local F = {}
for i = 1, #B do
  F[i] = B[i][2]
end

local R = run(F, 500, 1000)

io.write("return {\n")
for i = 1, #R do
  local v = R[i]
  io.write(format("{ %.17g, %.17g };\n", v[1], v[2]))
end
io.write("}\n")
